{
    "buildCommand": "cd frontend && npm install && npm run build",
    "outputDirectory": "frontend/dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/solve",
            "destination": "/api/index"
        },
        {
            "source": "/valuation",
            "destination": "/api/index"
        },
        {
            "source": "/structure",
            "destination": "/api/index"
        },
        {
            "source": "/simulate",
            "destination": "/api/index"
        },
        {
            "source": "/spot",
            "destination": "/api/index"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}